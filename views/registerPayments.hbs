<body onload="tablaRP()">

<div class="container" id="main" style="width: 100%; margin-top: -30px;">
    <div class="row">
        <div class="col-md-12" style="font-size: 22pt;">
            <label>Registro Alicuotas</label>
        </div> 
    </div>
    <div class="row">
        <div class="col-md-12" style="font-size: 12pt;">
            <label>Datos pago alícuotas</label> 
            {{{ifm message}}}
        </div> 
    </div>
     <form action="/registerC" method="POST">
    <div class="row">
        <div class="col-3" >
            <h5> #  <i class="fa fa-building"></i> o <i class="fa fa-home" aria-hidden="true"></i></h5>
            <select style="height: 33px;" class="form-control" name="numcasa" id="numcasa" onchange="getPE()">
                 <option value="#"  selected disabled>--Selecione Opcion--</option>
            </select>
           
        </div> 
        <div class="col-3" >
            <h5> Persona Encargada <i class="fa fa-users"></i></h5>
            <select style="height: 33px;" class="form-control" id="perenc"  name="perenc" required disabled>
                 <option value="#"  selected disabled>--Selecione Opcion--</option>
            </select>
           
        </div> 
        <div class="col-2" >
            <h5>Mes <i class="fa fa-calendar"></i></h5>  
            <select style="max-width: 150px;height: 33px;" class="form-control" id="mes" name="mes" required disabled>
                 {{{getMeses}}}
            </select>
           
        </div> 
         <div class="col-2" >
            <h5>Año <i class="fa fa-calendar"></i></h5>  
            <select style="max-width: 150px;height: 33px;" class="form-control" id="anio" name="anio" required disabled>
                 {{{ getAnios }}}
            </select>
           
        </div> 
    </div> 
    <div class="row">
        <div class="col-md-2 offset-md-12" >
            <h5>Monto <i class="fa fa-money"></i></h5>  
            <input  type="number" class="form-control"id="monto" name="monto" value="" placeholder="Monto" required disabled>
           
        </div>
        <div class="col-3" >
            <h5>Estado <i class="fa fa-info-circle"></i></h5>  
            <select style="max-width: 200px;height: 33px;" class="form-control" name ="estado" id="estado" required disabled>
                <option value="#" disabled>--Selecione Opcion--</option>
                 <option value="Pagado">Pagado</option>
                 <option value="Debe">Debe</option>
            </select>
           
        </div>
        <div class="col-md-2 offset-md-12" >
            <input type="submit" class="btn btn-success" id="boton" name="boton" value="Pagar" style="margin-bottom: -90px;" disabled> 
        </div>
    </div>
   </form>
    <hr style="max-width: 88%;margin-left: 0px;">
    <div class="row">
        <div class="col-md-12" style="font-size: 12pt;">
            <label>Tabla ultimos pagos alícuotas</label> 
        </div> 
    </div>
    <div class="row" style="width:90%">
	<table id="example" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>ID</th>
                <th># VIVIENDA</th>
                <th>NOMBRES</th>
                <th>APELLIDOS</th>
                <th>MONTO</th>
                <th>FECHA</th>
                <th>MES PAGO</th>
                <th>AÑO PAGO</th>
                <th>ESTADO</th>
            </tr>
        </thead>
        <tbody>
           
        </tfoot>
    </table>
	</div>

</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
{{> footer}}