                   <option value="#"  selected disabled>--Selecione Opcion--</option>
                </select>
            </div>          
        </div> 
        <div class="col-md-2 offset-md-12" >
            <input type="submit" class="btn btn-success" name="boton" value="Registro" style="margin-bottom: -70px;"> 
        </div>
    </div>
    </form>
